{
  "name": "custom-github-action",
  "version": "0.0.1",
  "description": "Creates custom github actions with folder structure and  dependencies in a single directory",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:3001 node dist/index.js",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tsc"
  },
  "author": "",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "type-fest": "^4.14.0",
    "zod": "^3.22.4"
  }
}
